{% extends 'base.html.twig' %}

{% block title %}Articles{% endblock %}

{% block body %}
    {% for article in articles %}
        <h2>{{article.title}}</h2>
        <p>{{article.content}}</p>
        <p>{{article.createdAt|date('Y-m-d')}}</p>
        <p>{{article.category}}</p>
        <p>{{article.author}}</p>
    {% endfor %}
{% endblock %}
